# Stunnel Configuration (TLS over TCP tunnel)
STUNNEL_PORT=443

# pgAdmin Configuration (optional, use with --profile admin)
PGADMIN_EMAIL=admin@example.com
PGADMIN_PASSWORD=admin_password
PGADMIN_PORT=5050
